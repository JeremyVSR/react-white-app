{
    "rules": {
        "class-name": true,
        "no-eval": true,
        "no-internal-module": true,
        "no-unsafe-finally": true,
        "no-var-keyword": true,
        "no-any": false,
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ]
    }
}